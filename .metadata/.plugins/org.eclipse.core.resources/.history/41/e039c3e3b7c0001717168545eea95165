package BuilderPattern;

import java.util.ArrayList;
import java.util.List;

public class Meal {

	private List<Item> items = new ArrayList<Item>();
	
	public void addItem(Item item){
		items.add(item);
		
	}
	public float getCost(){
		
		float cost = 0;
		for(Item i:items)
			cost+=i.price();
		
		return 0;
	}
}
